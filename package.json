{
  "name": "crisp-interview-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t crisp-frontend .",
    "docker:run": "docker run -p 3000:80 crisp-frontend",
    "deploy": "./deploy.sh production",
    "deploy:dev": "./deploy.sh development"
  },
